{
  "name": "Headline Fetcher",
  "manifest_version": 2,
  "version": "0.0.1",
  "description": "Retrieves headlines relevant to the website you are currently on",
  "content_scripts": [{
    "js": [ "content.js"],
    "matches": [ "<all_urls>"]
   }],
  "background": {
    "scripts": ["background.js"]
   },
   "icons": {
    "16": "newspaper.png",
    "48": "newspaper.png",
    "128": "newspaper.png"
  },
   "permissions": ["contextMenus", "tabs"],
   "web_accessible_resources" : ["*.html"]
}
